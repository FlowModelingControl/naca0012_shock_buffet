FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      nuTilda;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 6.1348e-5;

boundaryField
{
    inlet
    {
        type            freestream;
        freestreamValue uniform 6.1348e-5;
    }

    outlet
    {
        type            freestream;
        freestreamValue uniform 6.1348e-5;
    }

    airfoil
    {
        type            fixedValue;
        value           uniform 0;
    }

    front
    {
        type            cyclic;
    }

    back
    {
        type            cyclic;
    }
    
    processor
    {
        type  processor;
        value $internalField;
    }

    processorCyclic
    {
        type  processorCyclic;
        value $internalField;
    }
}

// ************************************************************************* //
